{
    "name": "magic-patterns-vite-template",
    "version": "0.0.1",
    "private": true,
    "type": "module",
    "scripts": {
        "dev:api": "vercel dev --listen 3001",
        "dev:frontend": "vite",
        "dev": "vite",
        "dev:full": "concurrently \"vercel dev --listen 3001\" \"vite\"",
        "build": "npx vite build",
        "lint": "eslint . --ext .js,.jsx,.ts,.tsx --quiet",
        "preview": "npx vite preview",
        "vercel:dev": "vercel dev --listen 3001",
        "test": "vitest",
        "analyze:knip": "knip --reporter markdown > .knip.md || exit 0",
        "analyze:depcheck": "depcheck --json > .depcheck.json || exit 0",
        "analyze:tsprune": "ts-prune -p tsconfig.json > .tsprune.txt || exit 0",
        "analyze:madge": "madge --extensions ts,tsx,js,jsx --orphans --warning --ts-config tsconfig.json src > .madge-orphans.txt || exit 0",
        "analyze": "npm run analyze:knip && npm run analyze:depcheck && npm run analyze:tsprune && npm run analyze:madge"
    },
