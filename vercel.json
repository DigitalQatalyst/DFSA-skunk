{
  "rewrites": [
    {
      "source": "/api/:path*",
      "destination": "https://dfsa-skunk-express-server-git-develop-digitalqatalysts-projects.vercel.app/api/:path*"
    },
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ],
  "framework": null,
  "buildCommand": "npm run build",
  "devCommand": "echo 'Skipping framework dev server'",
  "installCommand": "npm install",
  "outputDirectory": "dist",
  "functions": {
    "api/**/*.ts": {
      "memory": 1024,
      "maxDuration": 30
    },
    "api/**/*.js": {
      "memory": 1024,
      "maxDuration": 30
    }
  }
}
